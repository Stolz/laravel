    /**
     * Tests bogus module home page is accessible.
     *
     * @return void
     */
    public function testBogusModuleHomePage()
    {
        // User without permissions
        $route = route('bogus.home');
        $response = $this->rejectUnauthorizedRouteAccess($route, 'get');

        // User with permissions
        $response = $this->actingAs($this->admin)->get($route);
        $response->assertOk();
        $response->assertSee('Bogus module');
    }
